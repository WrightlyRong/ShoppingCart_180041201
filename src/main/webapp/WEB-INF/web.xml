<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginSuccessfulServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.LoginSuccessfulServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewShoppingCartServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.ViewShoppingCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCartServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.EditCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOutServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.CheckOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GoBackHomeServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.GoBackHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddItemCartServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.AddItemCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditItemQuantityServlet</servlet-name>
        <servlet-class>com.example.shoppingcart_180041201.EditItemQuantityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginSuccessfulServlet</servlet-name>
        <url-pattern>/LoginSuccessfulServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewShoppingCartServlet</servlet-name>
        <url-pattern>/ViewShoppingCartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCartServlet</servlet-name>
        <url-pattern>/EditCartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOutServlet</servlet-name>
        <url-pattern>/CheckOutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GoBackHomeServlet</servlet-name>
        <url-pattern>/GoBackHomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddItemCartServlet</servlet-name>
        <url-pattern>/AddItemCartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditItemQuantityServlet</servlet-name>
        <url-pattern>/EditItemQuantityServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>